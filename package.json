{
  "name": "google-play-scraper",
  "version": "10.0.0",
  "description": "scrapes app data from google play store",
  "type": "module",
  "exports": "./index.js",
  "scripts": {
    "test": "jest",
    "test:report": "jest --coverage",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:run:all": "npm run test && npm run test:coverage"
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/facundoolano/google-play-scraper"
  },
  "keywords": [
    "google",
    "play",
    "scraper"
  ],
  "author": "Facundo Olano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/facundoolano/google-play-scraper/issues"
  },
  "homepage": "https://github.com/facundoolano/google-play-scraper",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "debug": "^3.1.0",
    "got": "^11.8.6",
    "memoizee": "^0.4.14",
    "ramda": "^0.29.0",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "babel-jest": "^29.7.0",
    "chai": "^4.3.7",
    "eslint": "^8.46.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "jest": "^29.7.0",
    "promise-log": "^0.1.0",
    "sinon": "^15.2.0",
    "validator": "^13.9.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "html"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.js"
    ]
  }
}
